      <package name="userinfo" extends="struts-default" >
    	<action name="userinfo_*" class="userinfoAction" method="{1}">
   		 	<result name="loginUI" >/login.jsp</result>
    		<result name="login" type="redirect">/index.jsp</result>
    		<result name="logout" type="redirect">/login.jsp</result>
    		<result name="addUI" >/add.jsp</result>
    		<result name="add" type="redirect">/login.jsp</result>
    		<result name="updateP">/userupdate.jsp</result>
    		<result name="update" type="redirect">/userinfo_list2.action</result>
